{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "shell:default",
    "shell:allow-open",
    "http:default",
    "updater:default",
    "process:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://api.github.com/repos/YoanBLGR/Outil-formalites/*"
        },
        {
          "url": "https://github.com/YoanBLGR/Outil-formalites/releases/*"
        },
        {
          "url": "https://guichet-unique-demo.inpi.fr/*"
        },
        {
          "url": "https://api.guichet-unique.inpi.fr/*"
        },
        {
          "url": "https://procedures.inpi.fr/*"
        }
      ]
    }
  ]
}

